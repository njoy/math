Cell[CellGroupData[{
Cell["Bivarite Probability Density", "Section",
 CellChangeTimes->{{3.6684344866329193`*^9, 3.668434495254682*^9}}],

Cell["\<\
This isn\[CloseCurlyQuote]t really a bivariate probability density. It\
\[CloseCurlyQuote]s a series of probability density functions defined at \
specific conditions. We need to find the probability of an event when the \
condition lies somewhere other than an explicitly defined condition. \
\>", "Text",
 CellChangeTimes->{{3.6684344977991457`*^9, 3.668434507392061*^9}, {
  3.668434589369898*^9, 3.668434649590363*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"fencePlot", "[", 
   RowBox[{"funcs_", ",", 
    RowBox[{"{", 
     RowBox[{"x_", ",", "xmin_", ",", "xmax_"}], "}"}], ",", 
    RowBox[{"yvals_:", " ", "Automatic"}], ",", 
    RowBox[{"opts", ":", 
     RowBox[{"OptionsPattern", "[", "]"}]}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"yv", ",", "pgons"}], "}"}], ",", 
    RowBox[{
     RowBox[{"yv", "=", 
      RowBox[{"yvals", "/.", 
       RowBox[{"Automatic", "\[Rule]", 
        RowBox[{"Range", "[", 
         RowBox[{"Length", "[", "funcs", "]"}], "]"}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"pgons", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"ColorData", "[", "\"\<Rainbow\>\"", "]"}], "[", 
           RowBox[{"Rescale", "[", 
            RowBox[{"n", ",", 
             RowBox[{"{", 
              RowBox[{"1", ",", 
               RowBox[{"Length", "@", "funcs"}]}], "}"}]}], "]"}], "]"}], ",", 
          RowBox[{
           RowBox[{"Cases", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Plot", "[", 
               RowBox[{
                RowBox[{"funcs", "[", 
                 RowBox[{"[", "n", "]"}], "]"}], ",", 
                RowBox[{"{", 
                 RowBox[{"x", ",", "xmin", ",", "xmax"}], "}"}], ",", 
                RowBox[{"Filling", "\[Rule]", "Axis"}]}], "]"}], "//", 
              "Normal"}], ",", 
             RowBox[{"Polygon", "[", "__", "]"}], ",", "Infinity"}], "]"}], "/.", 
           RowBox[{
            RowBox[{"Polygon", "[", "a__", "]"}], "\[RuleDelayed]", 
            RowBox[{"Polygon", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"{", 
                RowBox[{"#1", ",", 
                 RowBox[{"yv", "[", 
                  RowBox[{"[", "n", "]"}], "]"}], ",", "#2"}], "}"}], "&"}], "@@@",
               "a"}], "]"}]}]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", 
          RowBox[{"Length", "@", "yv"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Graphics3D", "[", 
      RowBox[{"pgons", ",", 
       RowBox[{"Axes", "\[Rule]", "True"}], ",", "opts"}], "]"}]}]}], 
   "]"}]}]], "Input",
