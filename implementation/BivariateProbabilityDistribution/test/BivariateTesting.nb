Cell[TextData[{
 "This is how we (linearly) interpolate between two of the conditioned \
probabilities. \n\n",
 StyleBox["event",
  FontWeight->"Bold"],
 " is what we want the probability for\n",
 StyleBox["condition",
  FontWeight->"Bold"],
 " constrains the distribution\n",
 StyleBox["pdfl",
  FontWeight->"Bold"],
 " is the conditioned probability distribution function below the given \
condtion\n",
 StyleBox["pdfh",
  FontWeight->"Bold"],
 " is the conditioned probability distribution function above the given \
condtion\n",
 StyleBox["cl",
  FontWeight->"Bold"],
 " is the defined condition below the given condition\n",
 StyleBox["ch",
  FontWeight->"Bold"],
 " is the defined condition above the given condition"
}], "Text",
 CellChangeTimes->{{3.668434681082965*^9, 3.668434729790111*^9}, {
  3.66843484207395*^9, 3.6684348453621187`*^9}, {3.668434944141389*^9, 
  3.668435057271652*^9}, {3.668435233583061*^9, 3.6684352412428503`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"p", "[", 
   RowBox[{
   "event_", ",", " ", "condition_", ",", " ", "pdfl_", ",", "pdfh_", ",", 
    "cl_", ",", " ", "ch_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"pdfl", "[", "event", "]"}], "+", 
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{"pdfh", "[", "event", "]"}], "-", 
      RowBox[{"pdfl", "[", "event", "]"}]}], 
     RowBox[{"ch", "-", "cl"}]], 
    RowBox[{"(", 
     RowBox[{"condition", "-", "cl"}], ")"}]}]}]}]], "Input",
 CellChangeTimes->{{3.668428535705443*^9, 3.668428652150475*^9}, {
  3.668434660203727*^9, 3.668434665960487*^9}}],

